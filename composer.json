{
	"name": "pvc/struct",
	"description": "pvc data structures library",
	"type": "project",
	"authors": [
		{
			"name": "Doug Wilbourne",
			"email": "dougwilbourne@gmail.com"
		}
	],
	"license": "MIT",
	"config": {
		"php": "8.1"
	},
	"autoload": {
		"psr-4": {
			"pvc\\struct\\": "src/",
			"pvcTests\\struct\\": "tests/",
			"pvcExamples\\struct\\": "examples/"
		}
	},
	"scripts": {
		"phpstan": "vendor/bin/phpstan -c phpstan.neon"
	},
	"minimum-stability": "dev",
	"require": {
		"pvc/interfaces": "*",
		"pvc/err": "*",
		"pvc/storage": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^9",
		"squizlabs/php_codesniffer": "*",
		"friendsofphp/php-cs-fixer": "*",
		"phpstan/phpstan": "*",
		"pvc/testing-traits": "*"
	}
}
